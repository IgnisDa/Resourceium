{
  "name": "resourceium",
  "dockerComposeFile": "docker-compose.yml",
  "service": "development-machine",
  "workspaceFolder": "/workspace/Resourceium",
  "forwardPorts": [
    3000,
    8000,
    8534,
    8533,
    8080
  ],
  "postCreateCommand": "script.sh && ${containerWorkspaceFolder}/.devcontainer/setup-deps",
  "remoteEnv": {
    "DJANGO_SECRET_KEY": "yEPrCbAmUYOocagvSlXbUhqQJMoaIAEjszUrKznJmEJBnNJZVr",
    "DJANGO_DEBUG": "1",
    "DJANGO_DATABASE_ENGINE": "django.db.backends.postgresql",
    "DJANGO_DATABASE_NAME": "website-db",
    "DJANGO_DATABASE_USER": "username",
    "DJANGO_DATABASE_PASSWORD": "admin-password",
    "DJANGO_DATABASE_HOST": "localhost",
    "GIT_AUTHOR_NAME": "${localEnv:GIT_AUTHOR_NAME}",
    "GIT_AUTHOR_EMAIL": "${localEnv:GIT_AUTHOR_EMAIL}",
    "GIT_COMMITTER_NAME": "${localEnv:GIT_COMMITTER_NAME}",
    "GIT_COMMITTER_EMAIL": "${localEnv:GIT_COMMITTER_EMAIL}",
    "GITZER_DONT_START_BROWSER": "1",
    "CHOKIDAR_USEPOLLING": "1",
    "VISUAL": "code --wait",
    "DEVELOPR_DATABASE_NAMES": "server_db|website-db",
    "DEVELOPR_DATABASE_USERS": "username",
    "VUE_APP_BACKEND_URL": "http://127.0.0.1:8000/graphql/",
    "STORAGE_SERVER": "http://127.0.0.1:8080/",
    "API_URL": "http://127.0.0.1:8000/"
  },
  "extensions": [
